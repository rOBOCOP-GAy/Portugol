programa {
    caracter sexo
    funcao inicio() {
    escreva("Qual o seu SEXO? [M/F] \n")
    leia(sexo)
    limpa()

    se(sexo == 'M' ou sexo == 'm')
  {
    escreva("Você é o sexo masculino!")
  }
    senao 
    {

      se(sexo == "F" ou sexo == "f")
      {
          escreva("Você é do sexo feminino!")
      }

      senao
      {
        escreva("O sexo informado não foi reconhecido, tente novamente!")
      }

    }
  }
}
